


<form action="{{{formurl}}}" method="get" name="paramform">
<input type="hidden" name="id" value="{{id}}" />
<input type="hidden" name="from" value="{{from}}" />
<input type="hidden" name="to" value="{{to}}" />
<input type="hidden" name="groupid" value="{{groupid}}" />
<input type="hidden" name="view" value="course_{{subview}}" />

<table width="100%">
    <tr valign="top">
        <td width="50%">
            {{{toporderstr}}}: {{{orderselect}}}
        </td>
        <td width="50%">
            {{{toplengthstr}}}: {{{lengthselect}}}
        </td>
    </tr>
</table>
</form>
